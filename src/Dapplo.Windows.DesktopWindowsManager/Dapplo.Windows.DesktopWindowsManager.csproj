<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <Description>Dapplo.Windows.DesktopWindowsManager helps to use the windows dwm api</Description>
    <AssemblyTitle>Dapplo.Windows.DesktopWindowsManager</AssemblyTitle>
    <AssemblyName>Dapplo.Windows.DesktopWindowsManager</AssemblyName>
    <PackageId>Dapplo.Windows.DesktopWindowsManager</PackageId>
    <PackageTags>windows dwm</PackageTags>
    <TargetFrameworks>net471;netcoreapp3.0;netstandard2.0</TargetFrameworks>
    <UseWindowsForms>true</UseWindowsForms>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Dapplo.Windows.Common\Dapplo.Windows.Common.csproj" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net471' ">
    <Reference Include="PresentationCore" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Drawing" />
  </ItemGroup>
</Project>
